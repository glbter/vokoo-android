<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.findUsers.FindUsersFragment">

    <fragment
        android:id="@+id/myUserFragment"
        android:name="com.vookoapp.ui.userPreview.UserPreviewFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_user_preview" />

    <fragment
        android:id="@+id/user1Fragment"
        android:name="com.vookoapp.ui.userPreview.UserPreviewFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/myUserFragment"
        app:layout_constraintEnd_toEndOf="@+id/myUserFragment"
        app:layout_constraintStart_toStartOf="@+id/myUserFragment"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_user_preview" />

    <fragment
        android:id="@+id/user2Fragment"
        android:name="com.vookoapp.ui.userPreview.UserPreviewFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/myUserFragment"
        app:layout_constraintEnd_toStartOf="@+id/myUserFragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user1Fragment"
        tools:layout="@layout/fragment_user_preview" />

    <fragment
        android:id="@+id/user3Fragment"
        android:name="com.vookoapp.ui.userPreview.UserPreviewFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/user4Fragment"
        app:layout_constraintEnd_toStartOf="@+id/myUserFragment"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myUserFragment"
        tools:layout="@layout/fragment_user_preview" />

    <fragment
        android:id="@+id/user4Fragment"
        android:name="com.vookoapp.ui.userPreview.UserPreviewFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/myUserFragment"
        app:layout_constraintStart_toStartOf="@+id/myUserFragment"
        app:layout_constraintTop_toBottomOf="@+id/myUserFragment"
        tools:layout="@layout/fragment_user_preview" />

    <fragment
        android:id="@+id/user5Fragment"
        android:name="com.vookoapp.ui.userPreview.UserPreviewFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/user4Fragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/myUserFragment"
        app:layout_constraintTop_toBottomOf="@+id/myUserFragment"
        tools:layout="@layout/fragment_user_preview" />

    <fragment
        android:id="@+id/user6Fragment"
        android:name="com.vookoapp.ui.userPreview.UserPreviewFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/myUserFragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/myUserFragment"
        app:layout_constraintTop_toBottomOf="@+id/user1Fragment"
        tools:layout="@layout/fragment_user_preview" />


</androidx.constraintlayout.widget.ConstraintLayout>

</layout>